                        <li><h5><a href="/Home/VehiclesNotChecked?depot=3" style="color: red; font-weight: bold">@leedsNotChecked not checked</a></h5></li>
                    </ul>
                    @*<div style="width:100%">
                        <a href="/Home/VehicleDefectsDetail?depot=3" class="btn btn-primary btn-sm btn-block" role="button">View</a>
                    </div>*@
                </div>
            </div>
            <div class="card mb-4 shadow-sm">
                <div class="card-header">
                    <h4 class="my-0 font-weight-normal">Tyne</h4>
