@model List<VehicleManagement.Models.DiscontinueVehicleModel>
@{
    ViewData["Title"] = "UpcomingMOT";
}

<h1>Showing @Model.Count.ToString() vehicles</h1>
<table class="table table-dark table-striped" width="100%">
    <thead class="thead-dark">
        <tr>
            <th>Depot</th>
            <th>Vehicle Reg</th>
            <th>Make</th>
            <th>Model</th>
            <th>In Warranty</th>
            <th>Start Date</th>
            <th>End Date</th>
        </tr>
    </thead>
    @foreach (var subItem in Model)
    {
        <tr>
            <td>@subItem.Depot</td>
            <td>@subItem.VanRegistration.ToString()</td>
            <td>@subItem.Make</td>
            <td>@subItem.Model</td>
            <td>@subItem.InWarranty</td>
            <td>@subItem.StartDate.ToString().Replace("/", "-").Substring(0, 10)</td>
            <td>@subItem.EndDate.ToString().Replace("/", "-").Substring(0, 10)</td>
            <td><button type="button" class="btn btn-warning" onclick="loadUploadConfirmationModal('@subItem.VanRegistration.ToString()')">Upload Docs</button></td>
        </tr>
    }
</table>
<div id="upload">
</div>
